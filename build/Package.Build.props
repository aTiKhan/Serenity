<Project>
  <Import Project="$(MSBuildThisFileDirectory)Package.Version.props" />
  <PropertyGroup>
    <Authors>Volkan Ceylan</Authors>
    <Company>Serenity</Company>
    <Copyright>Copyright Â© Serenity Software, Volkan Ceylan</Copyright>
    <PackageIcon>package-icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/serenity-is/Serenity</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>Serenity</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <None Include="$(MSBuildThisFileDirectory)assets\package-icon.png" Pack="true" PackagePath="">
        <Link>Properties\package-icon.png</Link>
    </None>
    <None Include="$(MSBuildThisFileDirectory)assets\package-readme.md" Pack="true" PackagePath="\README.md">
        <Link>Properties\README.md</Link>
    </None>
  </ItemGroup>
</Project>